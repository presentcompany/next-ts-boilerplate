{
  "name": "next-ts",
  "version": "0.1.0",
  "private": true,
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "scripts": {
    "husky:init": "npx husky-init && yarn",
    "pre-commit": "lint-staged",
    "prettier:check": "prettier --check './src/**/*.{js,jsx,ts,tsx,json,md}'",
    "prettier:fix": "prettier --write './src/**/*.{js,jsx,ts,tsx,json,md}'",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint:es && yarn lint:styled",
    "lint:es": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx src/",
    "lint:es-fix": "eslint --fix --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:styled": "stylelint ./src/**/*.{css,scss,js,ts,tsx}",
    "lint:styled-fix": "stylelint ./src/**/*.{css,scss,js,ts,tsx} --fix",
    "lint:fix": "next lint --fix",
    "export": "next export",
    "post-install": "yarn run gen:theme-typings",
    "gen:sitemap": "next-sitemap",
    "gen:theme-typings": "chakra-cli tokens './src/theme/index.ts'",
    "cy:test-dev": "start-server-and-test 'yarn build && yarn start' 3000 'yarn cy:run-dev'",
    "cy:run-dev": "cypress run  --config baseUrl=http://localhost:3000",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:jest": "jest --passWithNoTests",
    "test:jest-since-dev": "jest --changedSince=develop",
    "test:jest-watch": "jest --watch",
    "test:prep": "yarn build && yarn start -p 3099",
    "test:e2e": "NODE_ENV=test start-server-and-test 'yarn start -p 3099' 3099 'yarn cy:open'",
    "storybook": "start-storybook --no-open -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "pkg:add": "sh ./scripts/yarn-add.sh",
    "pkg:del": "sh ./scripts/yarn-del.sh",
    "pkg:install": "sh ./scripts/yarn-smart-install.sh",
    "browserslist:update": "npx browserslist@latest --update-db"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.1",
    "@chakra-ui/react": "^2.1.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.9.0",
    "@types/deep-extend": "^0.4.32",
    "axios": "^0.27.2",
    "cypress-axe": "^0.14.0",
    "deep-extend": "^0.6.0",
    "framer-motion": "^6.3.10",
    "fuse.js": "^6.6.2",
    "joi": "^17.6.0",
    "next": "12.1.6",
    "next-seo": "^5.4.0",
    "qs": "^6.10.3",
    "rc-slider": "^10.0.0",
    "react": "18.1.0",
    "react-dom": "^18.1.0",
    "react-google-recaptcha": "^2.1.0",
    "react-hook-form": "^7.31.3",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-player": "^2.10.1",
    "react-query": "^3.39.1",
    "recoil": "^0.7.3"
  },
  "devDependencies": {
    "@axe-core/react": "^4.4.2",
    "@babel/core": "^7.18.2",
    "@chakra-ui/cli": "^2.0.0",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@emotion/babel-plugin": "^11.9.2",
    "@storybook/addon-a11y": "^6.5.6",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/addon-viewport": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.11",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/gtag.js": "^0.0.10",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.40",
    "@types/qs": "^6.9.7",
    "@types/react": "18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-google-recaptcha": "^2.1.5",
    "@types/react-mailchimp-subscribe": "^2.1.1",
    "@types/testing-library__cypress": "^5.0.9",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "cypress": "^10.0.2",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "^8.17.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-storybook": "^0.5.12",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^13.0.0",
    "next-sitemap": "^3.0.5",
    "postcss": "^8.4.14",
    "postcss-styled": "^0.34.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "start-server-and-test": "^1.14.0",
    "storybook-addon-performance": "^0.16.1",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "ts-jest": "^28.0.4",
    "typescript": "4.7.3",
    "webpack": "^5.73.0"
  }
}
