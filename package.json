{
  "name": "next-ts",
  "version": "0.1.0",
  "private": true,
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "scripts": {
    "husky:init": "npx husky-init && yarn",
    "pre-commit": "lint-staged",
    "prettier:check": "prettier --check './src/**/*.{js,jsx,ts,tsx,json,md}'",
    "prettier:fix": "prettier --write './src/**/*.{js,jsx,ts,tsx,json,md}'",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint:es && yarn lint:styled",
    "lint:es": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx src/",
    "lint:es-fix": "eslint --fix --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:styled": "stylelint ./src/**/*.{css,scss,js,ts,tsx}",
    "lint:styled-fix": "stylelint ./src/**/*.{css,scss,js,ts,tsx} --fix",
    "lint:fix": "next lint --fix",
    "export": "next export",
    "post-install": "yarn run gen:theme-typings",
    "gen:sitemap": "next-sitemap",
    "gen:theme-typings": "chakra-cli tokens './src/theme/index.ts'",
    "cy:test": "cypress run",
    "cy:open": "cypress open",
    "test:jest": "jest --passWithNoTests",
    "test:jest-since-dev": "jest --changedSince=develop",
    "test:jest-watch": "jest --watch",
    "storybook": "start-storybook --no-open -p 6006 ",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "pkg:add": "sh ./scripts/yarn-add.sh",
    "pkg:del": "sh ./scripts/yarn-del.sh",
    "pkg:install": "sh ./scripts/yarn-smart-install.sh"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.8.8",
    "@types/deep-extend": "^0.4.32",
    "axios": "^0.25.0",
    "cypress-axe": "^0.14.0",
    "deep-extend": "^0.6.0",
    "framer-motion": "^6.2.4",
    "fuse.js": "^6.5.3",
    "joi": "^17.6.0",
    "next": "12.1.0",
    "next-seo": "^5.1.0",
    "qs": "^6.10.3",
    "rc-slider": "^9.7.5",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "react-google-recaptcha": "^2.1.0",
    "react-hook-form": "^7.26.1",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-player": "^2.9.0",
    "react-query": "^3.34.14",
    "recoil": "^0.6.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.3.2",
    "@babel/core": "^7.17.2",
    "@chakra-ui/cli": "^1.8.1",
    "@emotion/babel-plugin": "^11.7.2",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-viewport": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/gtag.js": "^0.0.8",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.16",
    "@types/qs": "^6.9.7",
    "@types/react": "17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-google-recaptcha": "^2.1.3",
    "@types/react-mailchimp-subscribe": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "babel-loader": "^8.2.3",
    "cypress": "^9.5.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-storybook": "^0.5.7",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "next-sitemap": "^2.1.13",
    "postcss": "^8.4.6",
    "postcss-styled": "^0.34.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "storybook-addon-performance": "^0.16.1",
    "stylelint": "^14.4.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "ts-jest": "^27.1.3",
    "typescript": "4.5.5",
    "webpack": "^5.69.1"
  }
}
